<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hailstone.FXMLDocumentController">
    <children>
        <Button fx:id="goButton" layoutX="128.0" layoutY="125.0" onAction="#handleButtonAction" text="Click Me!" />
      <TextField fx:id="numberInput" layoutX="138.0" layoutY="75.0" prefHeight="25.0" prefWidth="45.0" />
      <Button fx:id="addOneButton" layoutX="210.0" layoutY="75.0" mnemonicParsing="false" onAction="#addOne" text="+ 1" />
      <Button fx:id="substractOneButton" layoutX="69.0" layoutY="75.0" mnemonicParsing="false" onAction="#substractOne" text="- 1" />
      <Button fx:id="addFiveButton" layoutX="251.0" layoutY="75.0" mnemonicParsing="false" onAction="#addFive" text="+ 5" />
      <Button fx:id="subtractFiveButton" layoutX="8.0" layoutY="75.0" mnemonicParsing="false" onAction="#substractFive" text="- 5" />
    </children>
</AnchorPane>
